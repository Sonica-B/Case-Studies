# Environment Variables for Docker Deployment
# Copy this file to .env and fill in your values

# HuggingFace Token for API access
HF_TOKEN=hf_YOUR_TOKEN_HERE

# Ngrok Authentication Token
# Get from: https://dashboard.ngrok.com/get-started/your-authtoken
NGROK_AUTHTOKEN=YOUR_NGROK_AUTH_TOKEN

# Optional: Custom ngrok domains (requires paid plan)
# Leave empty to use randomly generated URLs
NGROK_API_DOMAIN=
NGROK_LOCAL_DOMAIN=
NGROK_GRAFANA_DOMAIN=

# Port ranges for your group: 5000-5009 (10 consecutive ports)
# Grafana intentionally uses 3000 to satisfy the professor's monitoring requirements
API_GRADIO_PORT=5000
API_METRICS_PORT=8000
API_NODE_EXPORTER_PORT=9100
LOCAL_GRADIO_PORT=5003
LOCAL_METRICS_PORT=8001
LOCAL_NODE_EXPORTER_PORT=9101
PROMETHEUS_PORT=5006
GRAFANA_PORT=5007
# Reserved ports for future use: 5008, 5009
